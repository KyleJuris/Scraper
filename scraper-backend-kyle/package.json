{
  "name": "ads-scraper-playwright",
  "version": "1.0.0",
  "description": "Playwright-based ads scraper with Supabase integration",
  "type": "module",
  "scripts": {
    "session:save": "tsx scripts/save-session.ts",
    "session:save-auto": "tsx scripts/save-session-auto.ts",
    "scrape": "tsx src/index.ts",
    "dev": "tsx src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test:pw": "playwright test"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "csv-parse": "^5.5.2",
    "csv-stringify": "^6.6.0",
