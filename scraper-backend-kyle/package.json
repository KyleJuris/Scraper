    "express": "^4.18.2",
    "playwright": "^1.40.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "scraper",
    "playwright",
    "supabase",
    "ads",
    "facebook",
    "google"
  ],
